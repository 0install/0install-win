<Project>

  <PropertyGroup>
    <!-- Build configuration -->
    <LangVersion>9.0</LangVersion>
    <TargetFramework>net472</TargetFramework>
    <TargetPlatformVersion>10.0.10240.0</TargetPlatformVersion>
    <Nullable>annotations</Nullable>
    <AutoGenerateBindingRedirects>False</AutoGenerateBindingRedirects>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn><!-- Missing XML comments -->
    <ApplicationIcon>..\..\icon.ico</ApplicationIcon>

    <!-- Metadata -->
    <Product>Zero Install</Product>
    <Copyright>Copyright Bastian Eicher et al.</Copyright>
    <RepositoryUrl>https://github.com/0install/0install-win</RepositoryUrl>

    <!-- Deploy native libs to output -->
    <RuntimeIdentifier>win</RuntimeIdentifier>

    <!-- Dummy version number, overridden by GitVersion -->
    <Version>1.0.0-pre</Version>

    <!-- Dependency versions -->
    <NanoByteCommonVersion>2.11.2</NanoByteCommonVersion>
    <ZeroInstallVersion>2.23.0</ZeroInstallVersion>
  </PropertyGroup>

</Project>
